{
	"$schema": "vscode://schemas/color-theme",
	"name": "TMX Colors",
	"colors": {},
	"tokenColors": [
		{
			"scope": "meta.tag.xml",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "entity.name.tag.xml",
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"scope": "entity.other.attribute-name.xml",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "string.quoted.double.xml",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "string.quoted.single.xml",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "comment.block.xml",
			"settings": {
				"foreground": "#6a9955",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "punctuation.separator.comma.csv",
			"settings": {
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": "constant.numeric.tile0",
			"settings": {
				"foreground": "#808080",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "constant.numeric.tile1",
			"settings": {
				"foreground": "#569cd6"
			}
		},
		{
			"scope": "constant.numeric.tile2",
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"scope": "constant.numeric.tile3",
			"settings": {
				"foreground": "#dcdcaa"
			}
		},
		{
			"scope": "constant.numeric.tile4",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"scope": "constant.numeric.tile5",
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": "constant.numeric.tile6",
			"settings": {
				"foreground": "#c586c0"
			}
		},
		{
			"scope": "constant.numeric.tile7",
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"scope": "constant.numeric.tile8",
			"settings": {
				"foreground": "#4fc1ff"
			}
		},
		{
			"scope": "constant.numeric.tile9",
			"settings": {
				"foreground": "#f48771"
			}
		},
		{
			"scope": "definedAttribute",
			"settings": {
				"foreground": "#4ec9b0",
				"fontStyle": ""
			}
		},
		{
			"scope": "customAttribute",
			"settings": {
				"foreground": "#dcdcaa",
				"fontStyle": "underline"
			}
		}
	]
}

